#include <iostream>
#include <cmath>
using namespace std;

void largestTwo(int x, int y, int z, int &largest, int &secondLargest)
{
    largest = x;
     if (y > largest)
     {
        largest = y;
     }
     if (z > largest)
     {
         largest = z;
     }
    if (largest == x)
    {
         secondLargest = max(y, z);
    }
     else if (largest == y)
     {
         secondLargest = max(x, z);
     }
     else
     {
           secondLargest = max(x, y);
     }
}
int main()
{
  cout<<"Enter Three Numbers"<<endl;
  int x;  int y; int z;
  cin>>x>>y>>z;
   int largest, secondLargest;
    largestTwo(x, y, z, largest, secondLargest);
     cout << "Largest = " << largest << endl;
    cout << "Second Largest = " << secondLargest << endl;
    return 0;
}
